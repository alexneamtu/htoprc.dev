{
  "name": "htoprc-dev",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "pnpm": {
    "overrides": {
      "undici": ">=7.18.2"
    }
  },
  "scripts": {
    "dev": "pnpm --filter @htoprc/web --filter @htoprc/api run --parallel dev",
    "build": "pnpm run -r build",
    "test": "pnpm run -r test",
    "test:parser": "pnpm --filter @htoprc/parser test",
    "lint": "pnpm run -r lint",
    "typecheck": "pnpm run -r typecheck",
    "db:migrate": "pnpm --filter @htoprc/api db:migrate",
    "db:migrate:prod": "pnpm --filter @htoprc/api db:migrate:prod",
    "db:seed": "pnpm --filter @htoprc/api db:seed",
    "deploy": "pnpm run -r deploy"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitest/coverage-v8": "^4.0.17",
    "eslint": "^9.18.0",
    "globals": "^17.0.0",
    "prettier": "^3.8.0",
    "typescript": "^5.7.0"
  },
  "packageManager": "pnpm@10.28.0",
  "engines": {
    "node": ">=24.0.0"
  }
}
