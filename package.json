{
  "name": "htoprc-dev",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm run --parallel dev:*",
    "dev:web": "pnpm --filter @htoprc/web dev",
    "dev:api": "pnpm --filter @htoprc/api dev",
    "build": "pnpm run -r build",
    "test": "pnpm run -r test",
    "test:parser": "pnpm --filter @htoprc/parser test",
    "lint": "pnpm run -r lint",
    "typecheck": "pnpm run -r typecheck",
    "db:migrate": "pnpm --filter @htoprc/api db:migrate",
    "db:migrate:prod": "pnpm --filter @htoprc/api db:migrate:prod",
    "db:seed": "pnpm --filter @htoprc/api db:seed",
    "deploy": "pnpm run -r deploy"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.0",
    "typescript": "^5.4.0"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
